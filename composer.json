{
    "name": "r-kujawa/laravel-payment-gateway",
    "description": "A robust gateway that integrates with any payment procesor and Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Kujawa",
            "email": "bobkujawa@gmail.com"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest"
        ,
        "orchestra/testbench": "^6.21"
    },
    "require": {
        "stymiee/authnetjson": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "rkujawa\\LaravelPaymentGateway\\": "src/",
            "rkujawa\\LaravelPaymentGateway\\Database\\Seeders\\": "src/Database/Seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rkujawa\\LaravelPaymentGateway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rkujawa\\LaravelPaymentGateway\\PaymentServiceProvider"
            ],
            "aliases": {
                "PaymentService": "rkujawa\\LaravelPaymentGateway\\Facades\\PaymentService"
            }
        }
    }
}
